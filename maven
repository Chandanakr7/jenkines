pipeline {
    agent any
tools {
    maven 'maven'
}
stages {
        stage('pull') {
            steps {
                git 'https://github.com/Chandanakr7/jenkines.git'
            }
        }
}
 stages {
        stage('clean') {
            steps {
                sh 'mvn clean'
    }
}
}
 stages {
        stage('vlaidate') {
            steps {
                sh 'mvn validate'
}
}
}
 stages {
        stage('test') {
            steps {
                sh 'mvn test'
}
}
}
}
