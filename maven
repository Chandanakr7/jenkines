            steps {
                sh 'mvn clean'
    }
}
}
 stages {
        stage('vlaidate') {
            steps {
                sh 'mvn validate'
}
}
}
 stages {
        stage('test') {
            steps {
                sh 'mvn test'
}
}
}
}
